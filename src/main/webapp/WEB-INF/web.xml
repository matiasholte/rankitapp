<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Oracle Corporation//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">
 
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>addmatch</servlet-name>
        <servlet-class>com.comoyo.jelastic.AddMatch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ranking</servlet-name>
        <servlet-class>com.comoyo.jelastic.Ranking</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>addmatch</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addmatch</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ranking</servlet-name>
        <url-pattern>/ranking</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>